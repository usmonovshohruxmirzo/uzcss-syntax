{
  "name": "uzcss-syntax",
  "displayName": "UZCSS Syntax",
  "description": "Basic syntax highlighting for .uzcss files",
  "version": "0.0.1",
  "publisher": "@webbro-software",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "uzcss",
        "aliases": [
          "UZCSS"
        ],
        "extensions": [
          ".uzcss"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "uzcss",
        "scopeName": "source.uzcss",
        "path": "./syntaxes/uzcss.tmLanguage.json"
      }
    ]
  }
}
