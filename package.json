{
  "name": "uzcss-syntax",
  "displayName": "UZCSS Syntax",
  "publisher": "your_publisher_name",
  "description": "Syntax highlighting for UZCSS files",
  "version": "0.0.1",
  "main": "./extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "uzcss",
        "aliases": [
          "UZCSS"
        ],
        "extensions": [
          ".uzcss"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "uzcss",
        "scopeName": "source.uzcss",
        "path": "./syntaxes/uzcss.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "uzcss.applyColors",
        "title": "Apply UZCSS Colors"
      }
    ]
  },
  "dependencies": {
    "vscode": "^1.1.37"
  }
}
